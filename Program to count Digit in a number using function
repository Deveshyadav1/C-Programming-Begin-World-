#include<stdio.h>
int countdigit(int a)
{
    int count=0;
    do
    {
      a/=10;
      count++;
    }
    while(a!=0);
    {
        return count;
    }
}

int main()
{
    int num,result;
    printf("\nEnter Number:");
    scanf("%d",&num);
    result=countdigit(num);
    printf("\nDigits : %d",result);
    return 0;
}
